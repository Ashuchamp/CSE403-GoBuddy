{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "cache": {
          "disabled": true
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "cache": {
          "disabled": true
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false,
        "cache": {
          "disabled": true
        }
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://gobuddy-backend-qvci.onrender.com/api"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "jinkehan04@gmail.com",
        "ascAppId": "6756219826",
        "appleTeamId": "VUBCR3HVYF"
      }
    }
  }
}
