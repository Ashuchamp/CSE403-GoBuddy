{
  "expo": {
    "name": "GoBuddy",
    "slug": "go-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "primaryColor": "#007AFF",
    "entryPoint": "./index.js",
    "description": "Find activity buddies on campus. Connect with students for sports, study sessions, and social activities.",
    "scheme": "com.googleusercontent.apps.353229046135-c1cq6bijclsnk1tdsjf520od87idhgma",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kehan.jin.gobuddy",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to upload profile pictures and activity photos.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select profile pictures and activity photos.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby activities and buddies."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.gobuddy",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "804a047d-5409-47de-9ecf-df76b774fcd2"
      }
    }
  }
}
